{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "generated/**", ".next/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test:run": {
			"dependsOn": ["^build"]
		},
		"lint": {
			"dependsOn": ["^build"]
		}
	}
}
